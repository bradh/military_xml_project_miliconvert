                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="front_radius_in_metres"/>
                      <xsd:element name="hyphen"/>
                      <xsd:element name="rear_radius_in_metres"/>
                      <xsd:element name="hyphen"/>
                      <xsd:element name="starboard_radius_in_metres"/>
                      <xsd:element name="hyphen"/>
                      <xsd:element name="port_radius_in_metres"/>
                      <xsd:element name="hyphen"/>
                      <xsd:element name="bottom_radius_in_metres"/>
                      <xsd:element name="hyphen"/>
                      <xsd:element name="top_radius_in_metres"/>
                    </xsd:sequence>
                  </xsd:complexType>
                </xsd:element>
              </xsd:choice>
              <xsd:choice maxOccurs="1" minOccurs="1">
                <xsd:element name="platform_srad_code">
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="r_band_susceptibility_radhaz_designator"/>
                      <xsd:element name="t_band_susceptibility_radhaz_designator"/>
                      <xsd:element name="u_band_susceptibility_radhaz_designator"/>
                      <xsd:element name="v_band_susceptibility_radhaz_designator"/>
                      <xsd:element name="w_band_susceptibility_radhaz_designator"/>
                      <xsd:element name="y_band_susceptibility_radhaz_designator"/>
                      <xsd:element name="z_band_susceptibility_radhaz_designator"/>
                    </xsd:sequence>
                  </xsd:complexType>
                </xsd:element>
              </xsd:choice>
              <xsd:choice maxOccurs="1" minOccurs="0">
                <xsd:element name="aircraft_limitations" type="xsd:string"/>
              </xsd:choice>
            </xsd:sequence>
            <xsd:attribute name="set-seq" type="xsd:string" use="optional"/>
            <xsd:attribute name="pos" type="xsd:string" use="optional"/>
          </xsd:complexType>
        </xsd:element>
        <xsd:element maxOccurs="1" minOccurs="0" name="fuel_used_depending_on_speeds_employed">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element maxOccurs="1" minOccurs="0" name="amplification" type="xsd:string"/>
              <xsd:element maxOccurs="1" minOccurs="0" name="narrative" type="xsd:string"/>
