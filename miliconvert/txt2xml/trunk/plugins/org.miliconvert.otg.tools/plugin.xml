<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="genmappingapp"
         point="org.eclipse.core.runtime.applications">
      <application
            thread="any">
         <run
               class="org.miliconvert.otg.tools.GenerateXSDApplication">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.miliconvert.otg.tools.genmappingapp"
            name="extractfield">
         <property
               name="appName"
               value="extractfield">
         </property>
      </product>
   </extension>

</plugin>
